{
   "name": "dune/framework",
   "description":"The PHP Dune Framework !",
   "type":"library",
   "license":"MIT",
   "keywords": [
      "php framework",
      "framework",
      "Dune"
   ],
   "support": {
      "issues":"https://github.com/phpdune/framework/issues",
      "source":"https://github.com/phpdune/framework"
   },
   "authors": [
      {
         "name":"Abhishek B",
         "email":"phpdune@gmail.com"
      }
   ],
   "autoload": {
      "psr-4": {
         "Dune\\":"src/Dune/"
      },
      "files": [
         "src/Dune/Core/GearFunc.php"
      ]
   },
   "autoload-dev": {
      "psr-4": {
         "Dune\\Tests\\":"tests/"
      }
   },
   "require": {
      "php": "^8.0",
      "ext-ctype": "*",
      "ext-openssl": "*",
      "ext-session": "*",
      "ext-tokenizer": "*",
      "illuminate/container": "^10.13",
      "illuminate/database": "^10.11",
      "leafs/http": "^2.2",
      "phpoption/phpoption": "^1.9",
      "symfony/console": "^6.2",
      "symfony/dotenv": "^6.3",
      "symfony/var-dumper": "^6.2",
      "vlucas/phpdotenv": "^5.5"
   },
   "replace": {
      "dune/pine":"self.version",
      "dune/routing":"self.version",
      "dune/csrf":"self.version",
      "dune/session":"self.version",
      "dune/http":"self.version",
      "dune/console":"self.version",
      "dune/database":"self.version",
      "dune/core":"self.version"
   },
   "require-dev": {
      "phpstan/phpstan": "^1.10",
      "phpunit/phpunit": "^10.2"
   },
   "config": {
      "sort-packages":true,
      "allow-plugins": {
         "pestphp/pest-plugin": false
      }
   },
   "scripts": {
     "test": "phpunit"
   },
   "minimum-stability":"stable",
   
   "prefer-stable": true
}
